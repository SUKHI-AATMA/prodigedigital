/*/Cookie consent popup/*/
.popup-fixed {
    width: 880px;
    opacity: 1;
    border-radius: 35px;
    background-image: linear-gradient(236deg, #c96b86, #562359);
    position: fixed;
    right: calc((100% - 880px) / 2);
    bottom: 20px;
    z-index: 120;

    @include transition(all .4s ease-in-out);
}

.popup-fixed .popup-fixed-inner {
    display: block;
    width: 100%;
    padding: 2% 5%;
    text-align: center;
    position: relative;
}

.popup-fixed .popup-fixed-inner .p-inner-description {
    float: left;
    width: 75%;
    font-size: 14px;
    font-family: $Montserrat;
    color: #ffffff;
    margin-bottom: 12px;
    text-align: left;
    line-height: 20px;
}

.popup-fixed .popup-fixed-inner .p-inner-description a {
    color: #71ceea;
    font-family: $MontserratBold;
    display: inline-block;
    text-decoration: none;
}

.popup-fixed .popup-fixed-inner .p-inner-description a:hover {
    color: #71ceea;
    opacity: 0.8;
}

.popup-fixed .popup-fixed-inner .got-it-button {
    display: inline-block;
    /* min-width: 200px; */
    /* min-height: 40px; */
    border-radius: 30px;
    border: solid 2px #ffffff;
    font-size: 14px;
    text-align: center;
    color: #ffffff;
    /* line-height: 36px; */
    transition: ease-in-out all 0.5s;
    padding: 10px 25px;
    text-decoration: none;
}

.popup-fixed .popup-fixed-inner .got-it-button:hover {
    border: solid 2px #ffffff;
    background: #ffffff;
    color: #692f60;
}

.popup-fixed .popup-fixed-inner .cross-icon {
    display: inline-block;
    position: absolute;
    width: 20px;
    height: 20px;
    right: 8px;
    top: 8px;
    background: url(../img/cross-icon.png) no-repeat 50% 50%;
    z-index: 1;
    -webkit-transition: ease-in-out all 0.5s;
    -o-transition: ease-in-out all 0.5s;
    transition: ease-in-out all 0.5s;
}

.popup-fixed .popup-fixed-inner .cross-icon:hover {
    opacity: 0.8;
}

