/*play Button*/
.playButton {
    position: relative;

    &:after {
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        height: 36px;
        width: 36px;
        border: 2px solid rgba(255, 255, 255, 0.5);
        border-radius: 40px;
        z-index: 100;
        content: '';
    }

    &:before {

        content: '';
        height: 0;
        width: 0;
        display: block;
        border: 10px transparent solid;
        border-right-width: 0;
        border-left-color: rgba(255, 255, 255, 0.5);
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-3px, -50%);
        z-index: 100;
    }

}

