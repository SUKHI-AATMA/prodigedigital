@media (max-height: 800px) and (max-width: 1280px) {
    footer .footer-block li a {
        font-size: 16px;
    }

    footer .footer-block p {
        font-size: 18px;
    }

    footer .footer-block:last-child p {
        font-size: 22px;
    }

    .btn {
        font-size: 14px;
        padding: 8px 15px;
    }


    .scrollDownHolder {
        top: calc(100% - #{get-vw(130px)});
    }

    #sectionTitle {
        left: -#{get-vw1280(40px)};

        h2 {
            font-size: (get-vw1280(12px));
        }
    }

    .fp-viewing-1 #sectionTitle h2:nth-child(1),
    .fp-viewing-2 #sectionTitle h2:nth-child(2),
    .fp-viewing-3 #sectionTitle h2:nth-child(3) {
        height: 12px;
    }


}

@media (max-height: 1024px) and (max-width: 768px) {

    .more,
    .morelink {
        font-size: 18px;
    }
}

@media (max-width: 1024px) {
    .jsonChange {
        font-family: $MontserratLight;
        &:before {
            display: none;
        }
    }
    .fixedBanner {
        .banner {
            position: relative !important;
        }
    }

    .free-consultation-floating-wrapper {
        display: none !important;
    }

    .mob-hide {
        display: none !important;
    }

    .mob-show {
        display: block !important;
    }

    /*Cookie popup*/

    .popup-fixed {
        width: 100%;
        opacity: 1;
        border-radius: 0;
        background-image: linear-gradient(236deg, #c96b86, #562359);
        position: fixed;
        text-align: center;
        right: 0;
        bottom: 0;
        padding: 1%;
        z-index: 120;
        @include transition(all .4s ease-in-out);
    }

    .popup-fixed .popup-fixed-inner {
        display: block;
        width: 100%;
        padding: 10px 24px;
        text-align: center;
        position: relative;
    }

    .popup-fixed .popup-fixed-inner .p-inner-description {
        display: block;
        width: 100%;
        font-family: $Montserrat;
        font-size: 12px;
        font-weight: normal;
        font-style: normal;
        font-stretch: normal;
        line-height: normal;
        letter-spacing: normal;
        color: #ffffff;
        margin-bottom: 12px;
        text-align: left;
        line-height: 20px;
    }

    .popup-fixed .popup-fixed-inner .p-inner-description a {
        color: #76c48b;
        display: inline-block;
        text-decoration: none;

    }

    .popup-fixed .popup-fixed-inner .p-inner-description a:hover {
        color: #76c48b;
        opacity: 0.8;
    }

    .popup-fixed .popup-fixed-inner .got-it-button {
        display: inline-block;
        /*min-width: 200px;*/
        /*min-height: 40px;*/
        border-radius: 30px;
        border: solid 2px #ffffff;
        font-family: $Montserrat;
        font-size: 14px;
        text-align: center;
        color: #ffffff;
        /*line-height: 36px;*/
        transition: ease-in-out all 0.5s;
        text-decoration: none;
        width: 100%;
    }

    .popup-fixed .popup-fixed-inner .got-it-button:hover {
        border: solid 2px #ffffff;
        background: #ffffff;
        color: #692f60;
    }

    .popup-fixed .popup-fixed-inner .cross-icon {
        display: inline-block;
        position: absolute;
        width: 20px;
        height: 20px;
        right: 8px;
        top: 8px;
        background: url(../img/cross-icon.png) no-repeat 50% 50%;
        z-index: 1;
        -webkit-transition: ease-in-out all 0.5s;
        -o-transition: ease-in-out all 0.5s;
        transition: ease-in-out all 0.5s;
    }

    .popup-fixed .popup-fixed-inner .cross-icon:hover {
        opacity: 0.8;
    }

    /*Cookie popup*/
    .pageTitle {
        display: none;
    }

    footer {
        float: left;
        width: 100%;
        z-index: 8;
    }

    .footer-bottom {
        flex-direction: column;
        align-items: center;

        .legal-wrp {
            margin-bottom: 20px;
        }
    }

    .footer-bottom a {
        margin: 0 50px;
    }

    footer .container.content .row {
        flex-direction: column;
    }

    footer .line {
        width: 100%;
        height: 2px;
        margin: 30px 0;
        opacity: 0.7;
    }

    footer .footer-block li:last-child {
        margin-bottom: 0;
    }

    footer .footer-block .links-wrp {
        justify-content: flex-start;
    }

    footer .footer-block p {
        margin-bottom: 15px;
    }

    footer .footer-block:last-child p {
        margin-bottom: 25px;
    }

    .btn {
        padding: 10px 25px;
    }

    .svgLetterBox {
        width: 30% !important;
    }

    #social {
        display: none !important;
    }

    a.button {
        font-family: "Montserrat-Regular";
        border-radius: 50px;
        padding: 2vw 4vw;
        font-size: 3vw;
        width: auto;
    }

    .scrollDownHolder {
        display: none;
    }

    .navOpen #social {
        display: none;
    }

    .introText p .textGrad {
        color: #c96b86;
    }

    #sectionTitle1 {
        top: 14vw !important;
        left: -13vw !important;
    }

    #sectionTitle1 h2 {
        font-size: 3vw !important;
    }

    header .siteSubnav a.loginBtn:before {
        display: none;
    }

    header .siteSubnav a.loginBtn {
        /*border: 2px solid #562359;*/
        background: url(#{$imgPath}/images/login.png) no-repeat center;
        background-size: contain;
        line-height: 30px;
        text-align: center;
        overflow: inherit;
        /*background: transparent !important;*/
    }

    header .siteSubnav a.loginBtn .login-arrow-mobile {
        display: inline-block;
        width: 24px;
        height: 12px;
        margin-left: -18px;
        background: #ffffff;
    }

}

