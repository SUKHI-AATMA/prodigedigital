/* Login Modal sass */
.login-modal {
    position: fixed;
    left: 0px;
    right: 0px;
    top: 0px;
    bottom: 0px;
    background: rgba(255, 255, 255, 0.9);
    z-index: 999;
    display: none;

    &.in {
        display: block;
    }

    .login-modal-content {
        display: table;
        vertical-align: middle;
        height: 100%;
        width: 100%;
        text-align: center;

        .login-body {
            display: table-cell;
            height: 100%;
            width: 100%;
            vertical-align: middle;
            text-align: center;

            .login-container {
                width: 800px;
                margin: 0 auto;
                border-radius: 10px;
                box-shadow: 0 10px 20px 0 rgba(0, 0, 0, 0.15);
                background-color: #ffffff;
                padding: 40px 70px;
                text-align: left;
                position: relative;

                .login-title {
                    display: block;
                    font-size: 42px;
                    font-weight: bold;
                    font-style: normal;
                    font-stretch: normal;
                    line-height: normal;
                    letter-spacing: normal;
                    font-family: $MontserratLight;
                    color: #333333;
                }

                .form-details {
                    display: inline-block;
                    width: 450px;
                    padding-top: 40px;

                    .form-input {
                        position: relative;
                        display: block;
                        -moz-transition: all 0.25s cubic-bezier(0.53, 0.01, 0.35, 1.5);
                        -o-transition: all 0.25s cubic-bezier(0.53, 0.01, 0.35, 1.5);
                        -webkit-transition: all 0.25s cubic-bezier(0.53, 0.01, 0.35, 1.5);
                        transition: all 0.25s cubic-bezier(0.53, 0.01, 0.35, 1.5);
                        margin-bottom: 40px;
                    }

                    .form-input:first-of-type {
                        padding-top: 0;
                    }

                    .form-input:last-of-type {
                        padding-bottom: 0;
                    }

                    .form-input label {
                        transform-origin: left center;
                        font-size: 24px;
                        font-weight: bold;
                        font-style: normal;
                        font-stretch: normal;
                        line-height: normal;
                        letter-spacing: normal;
                        color: #bababa;
                        text-align: left;
                        box-sizing: border-box;
                        padding: 1px 0;
                        display: block;
                        position: absolute;
                        background: none;
                        top: 0px;
                        left: 0px;
                        z-index: 2;
                        pointer-events: none;
                        -moz-transition: all 0.25s cubic-bezier(0.53, 0.01, 0.35, 1.5);
                        -o-transition: all 0.25s cubic-bezier(0.53, 0.01, 0.35, 1.5);
                        -webkit-transition: all 0.25s cubic-bezier(0.53, 0.01, 0.35, 1.5);
                        transition: all 0.25s cubic-bezier(0.53, 0.01, 0.35, 1.5);
                    }

                    .form-input .input-type {
                        -webkit-appearance: none;
                        width: 100%;
                        display: block;
                        box-sizing: border-box;
                        padding: 0 0;
                        font-size: 24px;
                        font-weight: 800;
                        font-style: normal;
                        font-stretch: normal;
                        line-height: normal;
                        letter-spacing: normal;
                        color: #572359;
                        position: relative;
                        z-index: 1;
                        height: 40px;
                        border-radius: 0;
                        background: none;
                        border: 0px;
                        border-bottom: solid 1px #888888;
                    }

                    .form-input.success label {
                        color: #92c845;
                    }

                    .form-input.success .input-type {
                        border: solid 0px #92c845;
                    }

                    .form-input.error label {
                        color: #e33e25;
                    }

                    .form-input .invalid {
                        font-size: 12px;
                        font-weight: 300;
                        font-style: normal;
                        font-stretch: normal;
                        line-height: normal;
                        letter-spacing: normal;
                        color: #000000;
                        float: left;
                        width: 100%;
                        display: none;
                        padding-left: 0;
                    }

                    .form-input.error .invalid {
                        display: block;
                    }

                    .form-input.error .input-type {
                        border: solid 0px #e33e25;
                    }

                    .form-input .input-type:focus {
                        outline: none;
                    }

                    .form-input .input-type:focus~label {
                        -moz-transform: translate(0, -16px);
                        -ms-transform: translate(0, -16px);
                        -webkit-transform: translate(0, -16px);
                        transform: translate(0, -16px);
                        font-size: 12px;
                    }

                    .form-input .input-type:valid~label {
                        -moz-transform: translate(0, -16px);
                        -ms-transform: translate(0, -16px);
                        -webkit-transform: translate(0, -16px);
                        transform: translate(0, -16px);
                        font-size: 12px;
                    }

                    .form-input.mobile-no .input-type {
                        padding-left: 110px;
                    }

                    .form-input.mobile-no .contry-code {
                        box-sizing: border-box;
                        display: inline-block;
                        max-width: 120px;
                        min-width: 100px;
                        position: absolute;
                        left: 0px;
                        top: 0px;
                        padding: 0px 0;
                        font-size: 36px;
                        font-weight: 800;
                        font-style: normal;
                        font-stretch: normal;
                        line-height: normal;
                        letter-spacing: normal;
                        color: #572359;
                        z-index: 1;
                        height: 54px;
                        border-radius: 4px;
                        background: url(../../dist/images/down-cart.png) no-repeat 94% 50%;
                        background-size: 10px;
                        border: solid 0px #888888;
                        -webkit-appearance: none;
                        z-index: 1;
                        outline: none;
                        visibility: hidden;
                        opacity: 0;
                        -webkit-transition: ease-in-out all 0.5s;
                        -o-transition: ease-in-out all 0.5s;
                        transition: ease-in-out all 0.5s;
                    }

                    .form-input.mobile-no .input-type:focus~.contry-code,
                    .form-input.mobile-no .input-type:valid~.contry-code {
                        visibility: visible;
                        opacity: 1;
                    }

                    .check-box-input {
                        display: block;
                        width: 100%;
                        margin-bottom: 30px;
                    }

                    .check-box-input .check-box {
                        display: block;
                        position: relative;
                        padding-left: 45px;
                        margin-bottom: 0;
                        cursor: pointer;
                        font-size: 14px;
                        font-weight: normal;
                        font-style: normal;
                        font-stretch: normal;
                        line-height: 22px;
                        letter-spacing: normal;
                        color: #787878;
                        -webkit-user-select: none;
                        -moz-user-select: none;
                        -ms-user-select: none;
                        user-select: none;
                    }

                    .check-box-input .check-box a {
                        color: #333333;
                        float: right;
                        text-decoration: none;
                        margin-right: 50px;
                    }

                    /* Hide the browser's default checkbox */
                    .check-box input {
                        position: absolute;
                        opacity: 0;
                        cursor: pointer;
                        height: 0;
                        width: 0;
                    }

                    /* Create a custom checkbox */
                    .checkmark {
                        position: absolute;
                        top: 0;
                        left: 0;
                        height: 22px;
                        width: 22px;
                        background-color: transparent;
                        border: 2px solid #582459;
                        border-radius: 3px;
                    }

                    /* On mouse-over, add a grey background color */
                    .check-box:hover input~.checkmark {
                        background-color: #ffffff;
                    }

                    /* When the checkbox is checked, add a blue background */
                    .check-box input:checked~.checkmark {
                        background-color: #ffffff;
                    }

                    /* Create the checkmark/indicator (hidden when not checked) */
                    .checkmark:after {
                        content: "";
                        position: absolute;
                        display: none;
                    }

                    /* Show the checkmark when checked */
                    .check-box input:checked~.checkmark:after {
                        display: block;
                    }

                    .check-box input:checked~.checkmark {
                        background: #562359;
                    }

                    /* Style the checkmark/indicator */
                    .check-box .checkmark:after {
                        left: 5px;
                        top: 1px;
                        width: 6px;
                        height: 10px;
                        border: solid #ffffff;
                        border-width: 0 2px 2px 0;
                        -webkit-transform: rotate(45deg);
                        -ms-transform: rotate(45deg);
                        transform: rotate(45deg);
                    }

                    .login-btn-row {
                        display: block;
                        width: 100%;
                        text-align: left;
                        padding-bottom: 30px;
                        position: relative;
                        margin-bottom: 28px;

                        &:before {
                            position: absolute;
                            content: '';
                            width: 74px;
                            height: 2px;
                            background: #bababa;
                            left: 0px;
                            bottom: 0px;
                        }

                        .login-btn {
                            width: 194px;
                            display: inline-block;
                            height: 36px;
                            border-radius: 25px;
                            font-size: 14px;
                            font-weight: bold;
                            font-style: normal;
                            font-stretch: normal;
                            line-height: normal;
                            letter-spacing: normal;
                            text-align: center;
                            color: #ffffff;
                            text-decoration: none;
                            padding: 9px 0;
                            margin: 0px;
                        }
                    }

                    .social-btn-row {
                        display: block;
                        width: 100%;
                        text-align: left;
                        position: relative;
                        margin-bottom: 33px;

                        label {
                            font-size: 12px;
                            font-weight: 300;
                            font-style: normal;
                            font-stretch: normal;
                            line-height: normal;
                            letter-spacing: normal;
                            display: block;
                            color: #333333;
                            margin-bottom: 15px;
                        }

                        .login-linkedin-btn {
                            width: 194px;
                            display: inline-block;
                            height: 36px;
                            border-radius: 25px;
                            font-size: 14px;
                            font-weight: bold;
                            font-style: normal;
                            font-stretch: normal;
                            line-height: normal;
                            letter-spacing: normal;
                            text-align: center;
                            color: #ffffff;
                            text-decoration: none;
                            padding: 9px 0;
                            margin: 0px;
                            background: #007cba;
                            -webkit-transition: ease-in-out all .5s;
                            -o-transition: ease-in-out all .5s;
                            transition: ease-in-out all .5s;

                            &:hover {
                                background: #006fa7;
                            }
                        }
                    }

                    .signup-text {
                        display: block;
                        width: 100%;
                        font-size: 12px;
                        font-weight: normal;
                        font-style: normal;
                        font-stretch: normal;
                        line-height: normal;
                        letter-spacing: normal;
                        color: #787878;

                        a {
                            font-weight: bold;
                            color: #572359;
                            text-decoration: none;
                        }
                    }
                }

                .right-animation {
                    position: absolute;
                    right: 25px;
                    top: 110px;
                    width: 120px;
                    display: inline-block;
                    bottom: 70px;

                    .dots-img {
                        display: block;
                        width: 100%;
                        overflow: hidden;
                        height: 100%;

                        img {
                            width: auto;
                            ;
                            vertical-align: middle;
                        }
                    }

                    .position-arrow {
                        display: inline-block;
                        width: 24px;
                        height: 24px;
                        text-align: center;
                        position: absolute;
                        top: -60px;
                        right: 10px;
                        z-index: 2;
                    }

                    .triangleMove {
                        position: absolute;
                        width: 80px;
                        height: 66px;
                        left: -42px;
                        top: -30px;
                        @include transform-origin(center);

                        .triangleAnim {
                            @include animation(squareAnim 7s infinite linear);
                            width: 100%;
                            height: 100%;
                            max-width: 100%;
                        }
                    }

                    .squareMove {
                        position: absolute;
                        width: 80px;
                        height: 80px;
                        left: -25px;
                        bottom: -25px;
                        @include transform-origin(center);

                        .squareAnim {
                            @include animation(squareAnim 7s infinite linear);
                            width: 100%;
                            height: 100%;
                            max-width: 100%;
                        }
                    }

                    .circlePulse {
                        position: absolute;
                        width: 60px;
                        height: 60px;
                        right: -10px;
                        top: 50%;
                        margin-top: -30px;
                        border-radius: 50%;
                        @include transform-origin(center);

                        .circleAnim {
                            @include animation(circleAnim 4s infinite linear);
                            @include transform-origin(center);
                            width: 100%;
                            height: 100%;
                        }
                    }
                }
            }
        }
    }
}

