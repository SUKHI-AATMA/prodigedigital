.svgLetterBox {
    width: (get-vw(96px));
    height: (get-vw(120px));
    position: relative;
    @include transition(all .4s ease-in-out);

    svg {
        height: 100%;
    }

}

/*// Website SVG Title letters + Animations Ends*/



/*// Website Structure Starts*/
.container {
    width: 100%;
    position: relative;
}

.flex-wrp {
    display: flex;
    justify-content: space-between;
}

.mob-show {
    display: none !important;
}

#sectionTitle {
    position: fixed;
    width: auto;
    left: -(get-vw(35px));
    top: (get-vw(90px));
    overflow: hidden;
    @include transform-origin(100% 50%);
    @include transform(rotate(-90deg));
    z-index: 9;

    h2 {
        font-family: $MontserratLight;
        font-size: (get-vw(14px));
        color: $colorPrimary;
        height: 0;
        overflow: hidden;
        @include transition(all .4s ease-in-out);
        text-align: right;
    }
}

#sectionTitle1 {
    position: fixed;
    width: auto;
    left: 7%;
    top: 5.0%;
    overflow: hidden;
    z-index: 12;
    @include transition(all .4s ease-in-out);

    .secTitleSlide {
        position: relative;
    }

    h2 {
        font-family: $MontserratLight;
        font-size: 0.9vw;
        color: $colorPrimary;
        height: auto;
        overflow: hidden;
        @include transition(all .4s ease-in-out);
        margin: 0 0 0.3vw;
    }
}

.pageTitle {
    position: fixed;
    width: auto;
    left: 2.85%;
    top: 4.94792vw;
    opacity: 0;
    transform: rotate(90deg) translateY(-50%);
    z-index: 11;
    transform-origin: 0 50%;

    h2 {
        font-family: $MontserratLight;
        font-size: 14px;
        letter-spacing: 1px;
        text-transform: uppercase;
        color: $colorPrimary;
        height: (get-vw(15px));
        overflow: hidden;
        transform: rotate(180deg);
        @include transition(all .4s ease-in-out);
        text-align: right;
    }
}
