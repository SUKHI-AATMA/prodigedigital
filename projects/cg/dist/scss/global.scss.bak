@media only screen and (max-width : 1024px) and (orientation : landscape) {
    body {
        overflow: hidden;
        position: relative;

        &:before {
            z-index: 52;
            background: #fff;
            display: flex;
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            content: 'Please rotate your devise to portrait mode for better viewing.';
            align-items: center;
            justify-content: center;
            font-family: inherit;
            font-size: 22px;
        }
        &.allowLandscape:before{
            display: none;
        }

    }
}

