.fp-viewing-1 #sectionTitle h2:nth-child(1),
.fp-viewing-2 #sectionTitle h2:nth-child(2),
.fp-viewing-3 #sectionTitle h2:nth-child(3),
.fp-viewing-4 #sectionTitle h2:nth-child(4),
.fp-viewing-5 #sectionTitle h2:nth-child(5),
.fp-viewing-6 #sectionTitle h2:nth-child(6) {
    height: (get-vw(15px));
}





.intro {
    background: #fff;
    height: 100%;
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 125;

    span {
        width: 100%;
        height: 5px;
        position: fixed;
        top: 0;
        left: 0;

        &:before {
            background: linear-gradient(to right, #562359, #c96b86);
            float: left;
            width: 1%;
            height: 5px;
            position: fixed;
            top: 0;
            left: 0;
            z-index: 11;
            transition: all 9.5s;
            content: '';
        }
    }

    &.active span:before {
        width: 100%;
    }
}

.btn {
    text-transform: capitalize;
    text-decoration: none;
    font-size: 16px;
    font-family: inherit;
    padding: 10px 25px;
    line-height: 1;
    border: 2px solid #fff;
    border-radius: 20px;
    background: transparent;
    color: #fff;
    cursor: pointer;
}

.button {
    background-size: 300% 100%;
    display: inline-block;
    color: $colorWhite;
    font-family: $Montserrat;
    font-size: 14px;
    line-height: 1;
    margin: (get-vw(50px)) 0 0;
    padding: 10px 25px;
    text-decoration: none;
    text-align: center;
    /*width: (get-vw(180px));*/

    @include border-radius((get-vw(35px)));
    @include background-gradient-effect(to right, #562359, #c96b86, #c96b86, #562359);
    @include transition(all .4s ease-in-out);


    &:hover {
        background-position: 100% 0;
    }

    &.plainButton {
        background: none;
        border: 1px solid #fff;
        width: 150px;
    }

    &.whiteButton {
        background: #fff;
        width: 150px;
    }

    &.plainGradButton {
        -webkit-border-image: -webkit-gradient(linear, 100% 0, 0 0, from(#562359), to(#c96b86)) 1 100%;
        -webkit-border-image: -webkit-linear-gradient(right, #562359, #c96b86) 1 100%;
        -moz-border-image: -moz-linear-gradient(right, #562359, #c96b86) 1 100%;
        -o-border-image: -o-linear-gradient(right, #562359, #c96b86) 1 100%;
        border-image: linear-gradient(to right, #562359, #c96b86) 1 100%;

    }
}

.introText {
    position: relative;
    width: (get-vw(500px));
    overflow: hidden;
    z-index: 1;
    padding: 0 0 0 (get-vw(40px));

    .line {
        position: absolute;
        display: block;
        width: (get-vw(8px));
        height: 100%;
        left: 0;
        top: 0;
        background-color: $colorSecondary;
        z-index: 1;
    }

    p {
        font-family: $MontserratBold;
        font-size: (get-vw(64px));
        color: $colorPrimary;
        line-height: 120%;

        .textGrad {
            color: #562359;
            /*display: inline-block;*/
            overflow: hidden;
            position: relative;
            transition: all 0.3s linear;

            &:before {
                /*transform: translateX(-100%);*/
                @include transition(background-position .8s ease-in-out);
                background: linear-gradient(to right, transparent 0%, transparent 33.33%, #c96b86 33.33%, #c96b86 66.66%, transparent 66.66%, transparent 100%);
                background-position: 100% 0;
                background-size: 300%;
                position: absolute;
                content: '';
                float: left;
                width: 0%;
                height: 5px;
                /*background: #c96b86;*/
                bottom: 0;
            }

            &.animate {
                color: #c96b86;
                /*background-position: 100%;*/
            }

            &.animate:before {
                background-position: 0 0;
                width: 100%;
            }
        }
    }
}

.fontello:before {
    font-family: "fontello";
    font-style: normal;
    font-weight: normal;
    speak: none;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    line-height: 1;
}

.navOpen #social,
.navOpen #sectionTitle,
.navOpen .scrollDownHolder,
.navOpen #sectionTitle1,
.navOpen .pageTitle,
.navOpen .popup-fixed,
.navOpen #fp-nav {
    opacity: 0 !important;
}

.navOpen .navFooter {
    opacity: 1;
    transition-delay: 0.4s;
}

.navOpen footer {
    z-index: 1;
}

.fixedBanner {
    display: flex;
    position: relative;
    height: 100vh;

    span.loader {
        background: #fff;
        float: left;
        /*width: 0%;*/
        height: 5px;
        position: fixed;
        top: 0;
        left: 0;
        z-index: 11;
        /*transition: all 9.5s;*/
        content: '';
        @include animation(loader 5s infinite linear);
    }
}

.slideUp {
    opacity: 0;
    position: relative;
    top: 30px;
    @include transition(all .4s ease-in-out);

    &.complete {
        opacity: 1;
        top: 0;
    }
}

.jsonChange {
    position: relative;
    cursor: pointer;
    transition: all 0.5s;
    font-family: $MontserratBold;
}

.jsonChange:before {
    top: 100%;
    content: '';
    width: 100%;
    height: 4px;
    float: left;
    background: #333;
    position: absolute;
    transition: all 0.4s;
}

.jsonChange:hover:before,
.jsonChange.active:before {
    background: #db5967;
}

#fp-nav.fp-right {
        position: fixed;
        width: 0.78125vw;
        height: 0;
        right: (get-vw(40px));
        top: 50%;
        transform: translateY(-50%);
        z-index: 99;

        ul {
            li {
                margin: 7px 0;
                a {
                    width: auto;
                    height: auto;
                    span {
                        width: 8px;
                        height: 8px;
                        background: #eae6ef;
                        background-color: #eae6ef;
                    }

                    &.active {
                        span {
                            margin: -2px 0 0 -2px;
                            background: #562359;
                            background-color: #562359;
                            width: 8px;
                            height: 8px;
                        }
                    }
                }
                &:hover a {
                    span {
                        width: 12px;
                        height: 12px;
                        margin: -4px 0 0 -4px;
                        background: #562359;
                        background-color: #562359;
                    }
                }
            }
        }
    }
